<script>      
$(function(){
  var cn01Swiper = new Swiper ('.cn01Swiper', {
     speed:1100,
		autoplay:{
			delay:2200,
		},
        
    // 如果需要前进后退按钮
    navigation: {
      nextEl: '.cn01Swiper-button-next',
      prevEl: '.cn01Swiper-button-prev',
    },

	on:{
       slideChange: function(){
        var dex = this.activeIndex;
		if(dex=="0"){
			$(".cn01 .bot ul li").eq(0).addClass("active");
			$(".cn01 .bot ul li").eq(0).siblings().removeClass("active");
		}else if(dex=="1")  {
			$(".cn01 .bot ul li").eq(1).addClass("active");
			$(".cn01 .bot ul li").eq(1).siblings().removeClass("active");
		}else if(dex=="2"){
			$(".cn01 .bot ul li").eq(2).addClass("active");
			$(".cn01 .bot ul li").eq(2).siblings().removeClass("active");
		}else if(dex=="3"){
			$(".cn01 .bot ul li").eq(3).addClass("active");
			$(".cn01 .bot ul li").eq(3).siblings().removeClass("active");
		}else if(dex=="4"){
			$(".cn01 .bot ul li").eq(4).addClass("active");
			$(".cn01 .bot ul li").eq(4).siblings().removeClass("active");
		}
 },
 }


  })        



  $(".cn01 .bot ul li").mouseenter(function(){
			$(this).addClass("active");
			$(this).siblings().removeClass("active");
			var now = $(this).attr("id");
			switch(now)
   {
     case "0":
	 cn01Swiper.slideTo(0, 1000, false);
     break;
	 case "1":
	 cn01Swiper.slideTo(1, 1000, false);
     break;
	 case "2":
	 cn01Swiper.slideTo(2, 1000, false);
     break;
	 case "3":
	 cn01Swiper.slideTo(3, 1000, false);
     break;
	 case "4":
	 cn01Swiper.slideTo(4, 1000, false);
     break;
     default:
        alert("错误");
}





		})
	})
  </script>








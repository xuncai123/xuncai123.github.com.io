            $(".subF").eq(0).hide();
        })
        $(".subF").eq(1).find(".sub .conF ").click(function(){
    
            $(".subF").eq(1).hide();
        })
    
     $(".fixed-sub").show();
     $(".fixed-sub ul li").click(function(){
             var subdex = $(this).index();
             if(subdex==0){      
                 
                 $(".subF").eq(0).show();
                 $(".subF").eq(1).hide();
             }
             if(subdex==2){  
                 $(".subF").eq(1).show();
                 $(".subF").eq(0).hide();
             }
     })
         }

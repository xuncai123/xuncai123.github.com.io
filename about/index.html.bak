<script>
    $(function(){
        var i  = 0;
        var length =   $(".ab05 .bot .left ul li").length

        function add (){    
            if(i ==length )   {
            i = 0
            }     
            $(".ab05 .bot .left ul li").eq(i).addClass("on");
            $(".ab05 .bot .left ul li").eq(i).siblings().removeClass("on")
            $(".ab05 .bot  .right p").eq(i).css("opacity","1");
            $(".ab05 .bot  .right p").eq(i).siblings().css("opacity","0");
            i++;     
        }
       var timer = setInterval(add,1500)
        $(".ab05 .bot .left ul li").mouseenter(function(){
            clearInterval(timer)
            var index =$(this).index()
            $(this).addClass("on");
            $(this).siblings().removeClass("on")
            $(".ab05 .bot  .right p").eq(index).css("opacity","1");
            $(".ab05 .bot  .right p").eq(index).siblings().css("opacity","0");
        })
        
        
        $(".ab05 .bot .left ul li").mouseleave(function(){       
            timer = setInterval(add,1500)      
        })


    })
</script>






